{
  "name": "toolbox2",
  "description": "TADL Toolbox v2",
  "website": "https://www.tadl.org/",
  "require": {
    "plugins": [
      {
        "name": "postgres",
        "commands": ["postgres:create $APP", "postgres:link $APP $APP"]
      }, {
        "name": "memcached",
        "commands": ["memcached:create $APP", "memcached:link $APP $APP"]
      }
    ]
  },
  "scripts": {
    "dokku": {
      "postdeploy": "echo"
    }
  }
}
